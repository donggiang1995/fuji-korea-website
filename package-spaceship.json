  "description": "FUJI Global Korea Corporate Website - Spaceship Hosting Version",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "esbuild server/index.ts --bundle --platform=node --outfile=server/index.js --external:node_modules/*",
    "dev": "tsx server/index.ts",
    "db:push": "drizzle-kit push:mysql",
    "db:generate": "drizzle-kit generate:mysql",
    "spaceship:deploy": "npm run build && npm run spaceship:upload",
    "spaceship:upload": "echo 'Upload files via cPanel File Manager or FTP'"
